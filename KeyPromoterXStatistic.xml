<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Database">
        <Item>
          <IdeaActionID>ActivateDatabaseToolWindow</IdeaActionID>
          <ShortCut>'Alt+0'</ShortCut>
          <Description>Database</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Execute">
        <Item>
          <IdeaActionID>Console.Jdbc.Execute</IdeaActionID>
          <ShortCut>'Ctrl+Enter'</ShortCut>
          <Description>Execute</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Go to scope declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Go to scope declaration</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Open Console">
        <Item>
          <IdeaActionID>Jdbc.OpenConsole</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F10'</ShortCut>
          <Description>Open Console</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>